
<table class="table">
	<% if vocabularies.any? %>
    <tr><td colspan="2" class="alert alert-danger">Vocabularies</td></tr>
  <% end %>
<% vocabularies.each do |voca| %>
  <tr class="vocabulary">
    <td class="alert alert-success"><%= voca.keyword %></td>
    <td class="alert alert-info"><%= simple_format voca.explain %></td>
  </tr>
<% end %>
</table>