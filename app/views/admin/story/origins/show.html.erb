<%= render "btn" %>

<h2><%= @origin.title %><span class="label label-warning state-<%= @origin.id%>"><%= @origin.state %></span></h2>
<p><%= simple_format @origin.content %></p>

<% @origin.translations.each do |trans| %>
  <%= render "translation", translation: trans %>
<% end %>

<%= render "vocabularies", vocabularies: @origin.vocabularies %>

