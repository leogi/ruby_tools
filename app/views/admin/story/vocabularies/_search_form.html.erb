<%= search_form_for [:admin, @search], url: admin_story_origin_vocabularies_path(@origin) do |f| %>
  <table class="table">
  <tr class="field">
    <th><%= f.label :language_eq, "Language" %></th>
    <td><%= f.select :language_eq, Settings.languages.map{|s| [s, s]}, include_blank: true %></td>
  </tr>
  <tr class="field">
    <th><%= f.label :keyword_cont, "Title" %></th>
    <td><%= f.text_field :keyword_cont %></td>
  </tr>
  <tr class="actions"><th colspan="2"><%= f.submit "Search", class: "btn btn-warning" %></th></tr>
  </table>
<% end %>
